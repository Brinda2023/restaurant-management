{
  "kind": "collectionType",
  "collectionName": "order_details",
  "info": {
    "singularName": "order-detail",
    "pluralName": "order-details",
    "displayName": "Order Detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "menuItem": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::menu-item.menu-item"
    },
    "served": {
      "type": "boolean",
      "default": false
    },
    "total": {
      "type": "integer"
    },
    "price": {
      "type": "integer"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "orderDetails"
    }
  }
}
