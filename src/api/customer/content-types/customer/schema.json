{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true
    },
    "otp": {
      "type": "integer"
    },
    "phone": {
      "type": "biginteger",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "token": {
      "type": "uid"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    },
    "restaurants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::restaurant.restaurant",
      "mappedBy": "customers"
    }
  }
}
