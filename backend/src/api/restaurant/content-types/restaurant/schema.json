{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "singularName": "restaurant",
    "pluralName": "restaurants",
    "displayName": "Restaurant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "richtext",
      "required": true
    },
    "rating": {
      "max": 7,
      "required": true,
      "type": "integer",
      "min": 1
    },
    "timing": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Open",
        "Closed"
      ],
      "required": true
    },
    "serviceType": {
      "type": "string",
      "required": true
    },
    "customers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::customer.customer",
      "inversedBy": "restaurants"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "restaurant"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "restaurant"
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "restaurant"
    }
  }
}
